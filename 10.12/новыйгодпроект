    time.sleep(0.5)
    pyautogui.press("8")
    time.sleep(0.5)
    pyautogui.press("0")
    time.sleep(0.5)
    pyautogui.press("9")
elif(x == "дура" or "аболтус" or "олух"):
    pyautogui.alert(text="ну зачем так делать? :(",title=x,button="дальше")
    pyautogui.hotkey("win", "R")
    pyautogui.press("enter")
    time.sleep(0.5)
    pyautogui.write("cd desktop",interval=0.5)
    time.sleep(0.5)
    pyautogui.press("enter")
    time.sleep(0.5)
    pyautogui.write("color 02",interval=0.5)
    time.sleep(0.5)
    pyautogui.press("enter")
    time.sleep(0.5)
    pyautogui.write("cd c:\windows",interval=0.5)
    time.sleep(0.5)
    pyautogui.press("enter")
    time.sleep(0.5)
    pyautogui.write("tree",interval=0.5)
    time.sleep(0.5)
    pyautogui.press("enter")
