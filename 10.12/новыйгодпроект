import pyautogui
import time
pyautogui.alert(text="Привет!!!",title="???",button="дальше")
pyautogui.alert(text="как дела?",title="???",button="хорошо")
pyautogui.alert(text="у меня тоже!!!",title="???",button="это здорово!")
pyautogui.alert(text="придумай мне имя",title="???",button="хорошо")
x = str(input())
if (x == "евдоким"):
    pyautogui.alert(text="УРА! МНЕ ОЧЕНЬ НРАВИТЬСЯ!",title=x,button="дальше")
    pyautogui.alert(text="смотри как умею",title=x,button="дальше")
    time.sleep(0.15)
    pyautogui.moveTo(1252,13)
    pyautogui.click()
    time.sleep(0.15)
    pyautogui.moveTo(206,45)
    pyautogui.click()
    time.sleep(0.25)
    pyautogui.write("bongo cat",interval=0.25)
    time.sleep(0.25)
    pyautogui.press("enter")
    time.sleep(0.25)
    pyautogui.moveTo(218,264)
    time.sleep(0.25)
    pyautogui.click()
    time.sleep(0.25)
    pyautogui.press("7")
    time.sleep(0.5)
    pyautogui.press("3")
    time.sleep(0.5)
    pyautogui.press("3")
    time.sleep(0.5)
    pyautogui.press("5")
    time.sleep(0.5)
    pyautogui.press("6")
    time.sleep(0.5)
    pyautogui.press("5")
    time.sleep(0.5)
    pyautogui.press("3")
    time.sleep(0.5)
